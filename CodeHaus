[32m+[m[32m  const [loadingMore, setLoadingMore] = useState<boolean>(false);[m
[32m+[m[32m  const [error, setError] = useState<string | null>(null);[m
[32m+[m[32m  const [hasMore, setHasMore] = useState<boolean>(true);[m
