[32m+[m[32m      setIsInitialLoading(true);[m
[32m+[m[32m      const { details, hasNext } = await fetchPokemonBatch(0);[m
[32m+[m[32m      setPokemon(details);[m
[32m+[m[32m      setHasMore(hasNext);[m
[32m+[m[32m      setNextOffset(PAGE_SIZE);[m
[32m+[m[32m    } catch (err) {[m
[32m+[m[32m      setError(err instanceof Error ? err.message : 'Unexpected error');[m
[32m+[m[32m    } finally {[m
[32m+[m[32m      setIsInitialLoading(false);[m
[32m+[m[32m    }[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  const handleLoadMore = async () => {[m
[32m+[m[32m    if (loadingMore || !hasMore) {[m
[32m+[m[32m      return;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    try {[m
[32m+[m[32m      setError(null);[m
[32m+[m[32m      setLoadingMore(true);[m
[32m+[m[32m      const { details, hasNext } = await fetchPokemonBatch(nextOffset);[m
[32m+[m[32m      setPokemon((prev) => [...prev, ...details]);[m
[32m+[m[32m      setHasMore(hasNext);[m
[32m+[m[32m      setNextOffset((prev) => prev + PAGE_SIZE);[m
[32m+[m[32m    } catch (err) {[m
[32m+[m[32m      setError(err instanceof Error ? err.message : 'Unexpected error');[m
[32m+[m[32m    } finally {[m
