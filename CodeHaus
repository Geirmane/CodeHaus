[32m+[m
[32m+[m[32m  const fetchPokemonBatch = async (offset: number) => {[m
[32m+[m[32m    const listResponse = await fetch(`${POKE_API_URL}?offset=${offset}&limit=${PAGE_SIZE}`);[m
[32m+[m[32m    if (!listResponse.ok) {[m
[32m+[m[32m      throw new Error('Unable to load Pok√©mon list');[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    const listData: PokemonListResponse = await listResponse.json();[m
[32m+[m[32m    const detailResponses = await Promise.all([m
[32m+[m[32m      listData.results.map(async (pokemonSummary) => {[m
[32m+[m[32m        const detailResponse = await fetch(pokemonSummary.url);[m
[32m+[m[32m        if (!detailResponse.ok) {[m
[32m+[m[32m          throw new Error(`Unable to load data for ${pokemonSummary.name}`);[m
[32m+[m[32m        }[m
[32m+[m[32m        return (await detailResponse.json()) as PokemonDetail;[m
[32m+[m[32m      }),[m
[32m+[m[32m    );[m
[32m+[m
